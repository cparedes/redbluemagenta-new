---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
	<title>redbluemagenta</title>
	<link href="http://www.redbluemagenta.com/atom.xml" rel="self"/>
	<link href="http://www.redbluemagenta.com/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.redbluemagenta.com/</id>
	<author>
		<name>Christian Paredes</name>
		<email>cp@redbluemagenta.com</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://www.redbluemagenta.com{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://www.redbluemagenta.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
 
</feed>
